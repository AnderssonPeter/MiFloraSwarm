{
  "name": "miflora-gateway",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "echo Starting... && concurrently -k -r \"npm:serve\" \"npm:graphql-codegen-watch\"",
    "serve": "ng serve",
    "build": "ng build --prod && gulp compress",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "bundle-report": "ng build --stats-json && webpack-bundle-analyzer dist/miflora-gateway/stats-es2015.json",
    "compress": "gulp compress",
    "generate-graphql-client": "apollo client:codegen --config=src/app/api/graphql/codegen.yml --includes=src/app/api/graphql/operations/* --localSchemaFile=src/app/api/graphql/schema.graphql --outputFlat --target=typescript --useReadOnlyTypes T:\\test\\test.ts",
    "graphql-codegen": "graphql-codegen --config src/app/api/graphql/codegen.yml",
    "graphql-codegen-watch": "graphql-codegen --config src/app/api/graphql/codegen.yml --watch"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^9.1.11",
    "@angular/common": "^9.1.11",
    "@angular/compiler": "^9.1.11",
    "@angular/core": "^9.1.11",
    "@angular/forms": "^9.1.11",
    "@angular/platform-browser": "^9.1.11",
    "@angular/platform-browser-dynamic": "^9.1.11",
    "@angular/router": "^9.1.11",
    "@fortawesome/angular-fontawesome": "^0.6.0",
    "@fortawesome/fontawesome-svg-core": "^1.2.27",
    "@fortawesome/free-brands-svg-icons": "^5.12.1",
    "@fortawesome/free-regular-svg-icons": "^5.12.1",
    "@fortawesome/free-solid-svg-icons": "^5.12.1",
    "apexcharts": "^3.19.3",
    "apollo-angular": "^1.9.1",
    "apollo-angular-link-http": "^1.10.0",
    "apollo-cache-inmemory": "^1.6.0",
    "apollo-client": "^2.6.0",
    "apollo-link": "^1.2.11",
    "bulma": "^0.8.0",
    "bulma-switch": "^2.0.0",
    "bulmaswatch": "^0.7.5",
    "chart.js": "^2.9.3",
    "graphql": "^14.6.0",
    "graphql-tag": "^2.10.0",
    "ng-apexcharts": "^1.3.0",
    "rxjs": "~6.5.4",
    "tslib": "^1.11.1",
    "zone.js": "~0.10.3"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^0.901.10",
    "@angular/cli": "^9.1.10",
    "@angular/compiler-cli": "^9.1.11",
    "@angular/language-service": "^9.1.11",
    "@graphql-codegen/cli": "^1.15.4",
    "@graphql-codegen/time": "^1.15.4",
    "@graphql-codegen/typescript": "^1.15.4",
    "@graphql-codegen/typescript-apollo-angular": "^1.15.4",
    "@graphql-codegen/typescript-operations": "^1.15.4",
    "@types/jasmine": "^3.5.8",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "^12.12.29",
    "apollo": "^2.28.3",
    "brotli": "^1.3.2",
    "codelyzer": "^5.1.2",
    "concurrently": "^5.2.0",
    "gulp": "^4.0.2",
    "gulp-brotli": "^2.0.2",
    "gulp-zopfli-green": "^4.0.2",
    "jasmine-core": "~3.5.0",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~4.3.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~2.1.0",
    "karma-jasmine": "~2.0.1",
    "karma-jasmine-html-reporter": "^1.4.2",
    "ng-openapi-gen": "^0.12.1",
    "prettier": "^2.0.5",
    "protractor": "~5.4.3",
    "ts-node": "~8.3.0",
    "tslint": "~5.18.0",
    "typescript": "~3.7.5",
    "webpack-bundle-analyzer": "^3.6.0"
  }
}
